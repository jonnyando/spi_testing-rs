{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
            "args": ["--release"],
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "Rust: cargo build --release"
